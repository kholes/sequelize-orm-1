<html>
  <head>
    <title>Students</title>
  </head>
  <body>
    <h3>Students Data</h3>
    <p><a href='/student/add'>Add Student</a></p>
    <table>
      <th>ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>&nbsp</th>
      <th>&nbsp</th>
      <% student.forEach(i=>{%>
      <tr>
        <td><%=i.id%></td>
        <td><%=i.first_name%></td>
        <td><%=i.last_name%></td>
        <td><%=i.email%></td>
        <td><%=i.id%></td>
        <td><a href="/student/edit/<%=i.id%>">Edit</a></td>
        <td><a href="/student/delete/<%=i.id%>">Delete</a></td>
      </tr>
      <%})%>
  </body>
</html>
