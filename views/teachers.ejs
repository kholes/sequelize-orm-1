<html>
  <head>
    <title>Taechers</title>
  </head>
  <body>
    <h3>Teachers Data</h3>
    <table>
      <th>ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <% teacher.forEach(i=>{ %>
      <tr>
        <td><%=i.id%></td>
        <td><%=i.first_name%></td>
        <td><%=i.last_name%></td>
        <td><%=i.email%></td>
      </tr>
      <%})%>
  </body>
</html>
